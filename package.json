{
  "name": "camroulette-api",
  "version": "0.1.0",
  "description": "# Installation",
  "scripts": {
    "postinstall": "npm run dist",
    "server": "nodemon --watch src src/server/main.js",
    "workers": "nodemon --watch src src/workers/main.js",
    "dev": "parallelshell 'webpack-dev-server' 'npm run server' 'npm run workers'",
    "dist": "webpack --config webpack.dist.config.js",
    "start": "parallelshell 'pm2 start server/main.js' 'pm2 start server/workers/main.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyanofthemoon/CamRoulette-API.git"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "express": "^4.13.3",
    "parallelshell": "^2.0.0",
    "pm2": "^1.0.2",
    "redis": "^2.6.2",
    "socket.io": "^1.4.8",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "nodemon": "^1.10.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
