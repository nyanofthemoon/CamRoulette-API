    "url": "git+https://github.com/nyanofthemoon/CamRoulette-API.git"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "parallelshell": "^2.0.0",
    "pm2": "^1.0.2",
    "redis": "^2.6.2",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "express": "^4.14.0",
    "nodemon": "^1.10.0"
  }
}
